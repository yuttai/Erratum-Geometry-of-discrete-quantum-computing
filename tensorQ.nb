Cell[TextData[{
 "Given a number ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "\[Element]", 
    SubscriptBox["\[DoubleStruckCapitalF]", "p"]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox["cFNormInv$", TraditionalForm]]],
 " will find a number ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "\[Element]", 
    SubscriptBox["\[DoubleStruckCapitalF]", 
     SuperscriptBox["p", "2"]]}], TraditionalForm]]],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Alpha]", "*"], "\[Alpha]", " ", "z"}], "\[Equal]", 
    "1"}], TraditionalForm]]],
 ". \nThis function is useful because given a state ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}], 
    "\[RightAngleBracket]"}], TraditionalForm]]],
 ", if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"\[CapitalPsi]", "\[VerticalSeparator]", "\[CapitalPsi]"}], 
     "\[RightAngleBracket]"}], "\[Equal]", "z"}], TraditionalForm]]],
 ", then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Alpha]", 
            RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}]}]}], 
          "\[RightAngleBracket]"}], ")"}], "\[ConjugateTranspose]"}], 
       RowBox[{"(", 
        RowBox[{"\[Alpha]", 
         RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}]}]}]}], 
      "\[RightAngleBracket]"}], ")"}], "\[Equal]", 
    RowBox[{
     SuperscriptBox["\[Alpha]", "*"], "\[Alpha]", " ", "z"}], "\[Equal]", 
    "1"}], TraditionalForm]]],
 ". Therefore, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Alpha]", 
     RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}]}], 
    "\[RightAngleBracket]"}], TraditionalForm]]],
 " is a normalized state."
}], "Text",
