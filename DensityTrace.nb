Cell[BoxData[{\(SwapParts[expr_, \ pos1_, \ 
        pos2_]\  := \ \(ReplacePart[#, #, \ {pos1, pos2}, \ {pos2, pos1}] &\)[
        expr]\), "\n", 
    RowBox[{\(TraceSystem[D_, \ s_]\), ":=", " ", 
      RowBox[{"(", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{\(Qubits = Reverse[Sort[s]]\), ";", 
          "\[IndentingNewLine]", \(TrkM = D\), ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", \(z = \((\(Dimensions[Qubits]\)[\([1]\)] + 
                1)\)\), ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
            
            RowBox[{\(q = 1\), ",", \(q < z\), ",", \(q++\), ",", 
              "\[IndentingNewLine]", 
              
              RowBox[{\(n = Log[2, \((\(Dimensions[TrkM]\)[\([1]\)])\)]\), 
                ";", "\[IndentingNewLine]", \(M = TrkM\), ";", 
                "\[IndentingNewLine]", \(k = Qubits[\([q]\)]\), ";", 
                "\[IndentingNewLine]", 
                RowBox[{
                  StyleBox["If",
                    FontColor->RGBColor[0, 0, 1]], 
                  StyleBox["[",
                    FontColor->RGBColor[0, 0, 1]], 
                  RowBox[{
                    StyleBox[\(k \[Equal] n\),
                      FontColor->RGBColor[0, 0, 1]], 
                    StyleBox[",",
                      FontColor->RGBColor[0, 0, 1]], 
                    StyleBox["\[IndentingNewLine]",
                      FontColor->RGBColor[0, 0, 1]], 
                    StyleBox[\(\(TrkM = {};\)\[IndentingNewLine]
                      
                      For[p = 1, p < 2\^n + 1, 
                        p = p + 2, \[IndentingNewLine]\(TrkM = 
                            Append[TrkM, 
                              Take[M[\([p, All]\)], {1, 2\^n, 2}] + 
                                Take[M[\([p + 1, All]\)], {2, 2\^n, 
                                    2}]];\)\[IndentingNewLine]\ ]\),
                      FontColor->RGBColor[0, 0, 1]], ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{
                      RowBox[{"For", "[", 
                        
                        RowBox[{\(j = 0\), ",", \(j < \((n - k)\)\), 
                          ",", \(j++\), ",", "\[IndentingNewLine]", 
                          RowBox[{\(b = {0}\), ";", "\[IndentingNewLine]", 
                            
                            StyleBox[\(For[i = 1, 
                                i < 2\^n + 
                                    1, \(i++\), \[IndentingNewLine]If[\((Mod[\
\((\(IntegerDigits[i - 1, 2, n]\)[\([n]\)] + \(IntegerDigits[i - 1, 2, 
                                        n]\)[\([n - j - 1]\)])\), 2])\) == 
                                      1\  && \ Count[b, \ i]\ \  \[Equal] 
                                      0, \ Permut = {i, \((FromDigits[
                                        SwapParts[\((IntegerDigits[i - 1, 
                                        2, \ n])\), {n}, {n - j - 1}], 2] + 
                                        1)\)}; \[IndentingNewLine]b = 
                                    Append[
                                      b, \((FromDigits[
                                        SwapParts[\((IntegerDigits[i - 1, 
                                        2, \ n])\), {n}, {n - j - 1}], 2] + 
                                        1)\)]; \[IndentingNewLine]c = 
                                    Range[2\^n]; \[IndentingNewLine]perm = 
                                    SwapParts[
                                      c, {i}, {\((FromDigits[
                                        SwapParts[\((IntegerDigits[i - 1, 
                                        2, \ n])\), {n}, {n - j - 1}], 2] + 
                                        1)\)}]; \[IndentingNewLine]\
\[IndentingNewLine]M = M[\([perm, 
                                        perm]\)];\[IndentingNewLine]\
\[IndentingNewLine]\ ]\ \ \ \ \[IndentingNewLine]]\),
